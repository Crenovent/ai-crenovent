{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "Bias Check Result Schema",
    "type": "object",
    "properties": {
      "check_id": {
        "type": "string",
        "description": "Unique bias check identifier"
      },
      "model_id": {
        "type": "string",
        "description": "Model identifier"
      },
      "industry": {
        "type": "string",
        "enum": ["SaaS", "Banking", "Insurance", "FS", "E-commerce"]
      },
      "metric_results": {
        "type": "object",
        "description": "Results for each bias metric"
      }
    },
    "required": ["check_id", "model_id", "industry", "metric_results"]
  }